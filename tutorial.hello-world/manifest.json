{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_popup": "hello.html",
    "default_icon": {
      "16" : "images/healthy_habits16.png",
      "32" : "images/healthy_habits32.png",
      "48" : "images/healthy_habits48.png",
      "128" : "images/healthy_habits128.png"
    }
  },
  "background" : {
    "service_worker": "service-worker.js"
  }
}
